{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.1.3",
        "nunomaduro/termwind": "^1.13"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["ironman-cli"]
}
